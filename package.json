    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/file-type": "^10.9.1",
    "@types/http-errors": "^1.8.0",
    "@types/http-graceful-shutdown": "^2.3.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^13.11.1",
    "@types/sharp": "^0.26.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@vercel/ncc": "^0.24.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "gts": "^2.0.2",
