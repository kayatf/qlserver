{
  "name": "qlserver",
  "version": "1.0.0",
  "description": "LDAP authenticated microservice for Brother QL-series label printers.",
  "main": "build/src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "start:dev": "cross-env NODE_ENV=development node .",
    "copy-files": "copyfiles -u 1 src/public/**/* build/src/",
    "watch": "tsc-watch --onSuccess \"run-s copy-files start:dev\"",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "gts clean",
    "check": "gts check",
    "fix": "gts fix"
  },
  "keywords": [],
  "author": "Riegler Daniel",
  "license": "MIT",
  "dependencies": {
    "@naturalcycles/json2html": "^1.1.0",
    "activedirectory2": "^2.1.0",
    "adm-zip": "^0.4.16",
    "body-parser": "^1.19.0",
    "content-type": "^1.0.4",
    "cors": "^2.8.5",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "file-type": "^15.0.1",
    "helmet": "^4.1.1",
    "http-errors": "^1.8.0",
    "http-graceful-shutdown": "^2.3.2",
    "lookpath": "^1.1.0",
    "morgan": "^1.10.0",
    "raw-body": "^2.4.1",
    "sharp": "^0.26.1",
    "swagger-ui-express": "^4.1.4",
